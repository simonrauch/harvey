{
  "name": "@simonrauch/harvey",
  "version": "2.4.3",
  "description": "A simple CLI for Harvest.",
  "repository": {
    "type": "git",
    "url": "git://github.com/simonrauch/harvey.git"
  },
  "bugs": {
    "url": "https://github.com/simonrauch/harvey/issues"
  },
  "keywords": [
    "harvest",
    "cli"
  ],
  "author": "Simon Rauch <sr@simonrauch.com>",
  "license": "MIT",
  "bin": {
    "harvey": "./build/cli/index.js"
  },
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "prettier": "prettier --check './**/*.ts'",
    "prettier:fix": "prettier --write './**/*.ts'",
    "eslint": "eslint . --ext .ts",
    "eslint:ci": "eslint --output-file eslint-report.json --format json . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "build": "rm -rf build && npx tsc && chmod +x build/cli/index.js",
    "dev": "npm run build && npx tsc --watch",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "test:ci": "mocha -r ts-node/register 'tests/**/*.ts' --reporter json > mocha-test-results.json",
    "prepare": "husky install; npm run build",
    "package": "ncc build --source-map"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "cli-table": "^0.3.11",
    "exceljs": "^4.3.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cli-table": "^0.3.0",
    "@types/filesystem": "^0.0.32",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-nock": "^1.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nock": "^13.2.4",
    "prettier": "2.6.0",
    "semantic-release": "^19.0.2",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "release": {
    "branches": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/exec",
        {
          "prepare": "npm run package"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
