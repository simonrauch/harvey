{
  "name": "@simonrauch/harvey",
  "version": "2.1.2",
  "description": "A simple CLI for Harvest.",
  "bin": {
    "harvey": "./build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/simonrauch/harvey.git"
  },
  "bugs": {
    "url": "https://github.com/simonrauch/harvey/issues"
  },
  "scripts": {
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "build": "rm -rf build; npx tsc",
    "prepare": "npm run build",
    "pkg:macos-x64": "pkg . --targets node17-macos-x64 --output pkg/macos-x64/harvey",
    "pkg:linux-x64": "pkg . --targets node17-linux-x64 --output pkg/linux-x64/harvey"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "keywords": [
    "harvest",
    "cli"
  ],
  "author": "Simon Rauch <sr@simonrauch.com>",
  "license": "MIT",
  "dependencies": {
    "exceljs": "^4.3.0",
    "node-harvest-api": "^1.0.6",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/filesystem": "^0.0.32",
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.8",
    "pkg": "^5.5.2",
    "prettier": "2.5.1",
    "typescript": "^4.6.2"
  }
}
