{
  "name": "@simonrauch/harvey",
  "version": "2.0.0",
  "description": "CLI to work with Harvest.",
  "bin": {
    "harvey": "./build/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/simonrauch/harvey.git"
  },
  "bugs": {
    "url": "https://github.com/simonrauch/harvey/issues"
  },
  "scripts": {
    "build": "npx tsc",
    "pkg:macos-x64": "npm run build && pkg . --targets node17-macos-x64 --output build/macos-x64/harvey",
    "pkg:linux-x64": "npm run build && pkg . --targets node17-linux-x64 --output build/linux-x64/harvey"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "keywords": [
    "harvest"
  ],
  "author": "Simon Rauch <sr@simonrauch.com>",
  "license": "MIT",
  "dependencies": {
    "node-harvest-api": "^1.0.6",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/filesystem": "^0.0.32",
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.8",
    "pkg": "^5.5.2",
    "typescript": "^4.6.2"
  }
}
