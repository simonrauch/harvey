{
  "name": "@simonrauch/harvey",
  "version": "2.3.9",
  "description": "A simple CLI for Harvest.",
  "bin": {
    "harvey": "./build/cli.js"
  },
  "files": [
    "build/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/simonrauch/harvey.git"
  },
  "bugs": {
    "url": "https://github.com/simonrauch/harvey/issues"
  },
  "scripts": {
    "prettier": "prettier --check 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "build": "rm -rf build && npx tsc && chmod +x build/cli.js",
    "dev": "npm run build && npx tsc --watch",
    "prepare": "husky install; npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "harvest",
    "cli"
  ],
  "author": "Simon Rauch <sr@simonrauch.com>",
  "license": "MIT",
  "dependencies": {
    "cli-table": "^0.3.11",
    "exceljs": "^4.3.0",
    "node-harvest-api": "github:simonrauch/node-harvest-api",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/filesystem": "^0.0.32",
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "prettier": "2.5.1",
    "typescript": "^4.6.2"
  }
}
